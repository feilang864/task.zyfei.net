@model FFLTask.SRV.ViewModel.Project.JoinModel
@Html.Partial("_JoinLeaf", Model)
@if (Model.Children != null && Model.Children.Count > 0)
{
    <ul style="padding-left: 25px;">
        @for (int i = 0; i < Model.Children.Count; i++)
        {
            <li><span style="padding-left: 12px;">
                @Html.Partial("_JoinTree", Model.Children[i])</span></li>
        }
    </ul>
}