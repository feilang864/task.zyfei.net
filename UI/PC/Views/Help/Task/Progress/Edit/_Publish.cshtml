            function refreshProject(event) {
                var projectId = get_current_project_id($(event));
                canPublish(projectId);
                refreshOwner(projectId, "#TaskItem_Owner_Id");
                refreshDifficulties(projectId, "#TaskItem_Difficulty");
                refreshPriority(projectId, "#TaskItem_Priority");
